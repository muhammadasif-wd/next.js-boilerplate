{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:eslint": "eslint '**/*.{ts,tsx}'",
    "lint:fix": "eslint --fix '**/*.{ts,tsx}'",
    "format": "prettier --check '**/*.{ts,tsx}'",
    "format:fix": "prettier --write '**/*.{ts,tsx}'",
    "prepare": "husky",
    "preview": "vite preview"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": []
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "next": "14.1.3",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "husky": "^9.0.11",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
